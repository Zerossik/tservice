import { createBrowserRouter } from "react-router-dom";
// components
import { Layout } from "../components/Layout";
import { ErrorPage } from "../pages/ErrorPage/ErrorPage";

export const router = createBrowserRouter(
  [
    {
      path: "/",
      element: <Layout />,
      errorElement: <ErrorPage />,
    },
  ],
  { basename: "/tservice" }
);
